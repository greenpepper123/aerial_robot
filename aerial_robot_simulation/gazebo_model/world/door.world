<?xml version="1.0" ?>
<sdf version='1.4'>
  <world name='default'>

    <physics type='ode'>
      <max_step_size>0.001</max_step_size>
      <real_time_factor>1</real_time_factor>
      <real_time_update_rate>1000</real_time_update_rate>
      <gravity>0 0 -9.8</gravity>
    </physics>

    <spherical_coordinates>
      <surface_model>EARTH_WGS84</surface_model>
      <latitude_deg>0</latitude_deg>
      <longitude_deg>0</longitude_deg>
      <elevation>0</elevation>
      <heading_deg>0</heading_deg>
    </spherical_coordinates>


    <!-- A global light source -->
    <scene>
        <sky>
          <clouds>
            <speed>12</speed>
          </clouds>
        </sky>
    </scene>

    <include>
      <uri>model://sun</uri>
    </include>

    <model name="ground_plane">
      <static>true</static>
      <link name="link">
        <collision name="collision">
          <geometry>
            <plane>
              <normal>0 0 1</normal>
              <size>100 100</size>
            </plane>
          </geometry>
          <surface>
            <friction>
              <ode>
                <mu>10</mu>
                <mu2>5</mu2>
              </ode>
            </friction>
          </surface>
        </collision>
        <visual name="visual">
          <cast_shadows>false</cast_shadows>
          <geometry>
            <plane>
              <normal>0 0 1</normal>
              <size>100 100</size>
            </plane>
          </geometry>
          <material>
            <script>
              <uri>model://asphalt_plane/materials/scripts</uri>
              <uri>model://asphalt_plane/materials/textures</uri>
              <name>vrc/asphalt</name>
            </script>
          </material>
        </visual>
      </link>
    </model>

    <!-- door -->
    <model name="slidoor">
      <self_collide>true</self_collide>
      <link name="slidoor_base">
        <pose>1.0 -1.5 1 0 0 0</pose>
        <collision name="col1">
          <pose>0 0 0 0 0 0</pose>
          <geometry>
            <box>
              <size>1.0 0.9 2.0</size>
            </box>
          </geometry>
        </collision>

        <visual name="vis1">
          <pose>0 0 0 0 0 0</pose>
          <geometry>
            <box>
              <size>1.0 0.9 2.0</size>
            </box>
          </geometry>
        </visual>

        <inertial>
          <pose>0 0 0 0 0 0</pose>
          <mass>10</mass>
          <inertia>
            <ixx>1</ixx>
            <ixy>0</ixy>
            <ixz>0</ixz>
            <iyy>1</iyy>
            <iyz>0</iyz>
            <izz>1</izz>
          </inertia>
        </inertial>
      </link>

      <link name="slidoor">
        <pose>0 -2.1 1 0 0 0</pose>
        <collision name="col2">
          <pose>0 0.05 0 0 0 0</pose>
          <geometry>
            <box>
              <size>1.0 0.1 2.0</size>
            </box>
          </geometry>
          <surface>
            <bounce>
              <restitution_coefficient>0.2</restitution_coefficient>
              <threshold>0.05</threshold>
            </bounce>
            <contact>
              <ode>
                <max_vel>10</max_vel>
              </ode>
            </contact>
            <friction>
              <ode>
                <mu>1</mu>
                <mu2>1</mu2>
              </ode>
            </friction>
          </surface>
        </collision>

        <visual name="vis2">
          <pose>0 0.05 0 0 0 0</pose>
          <geometry>
            <box>
              <size>1.0 0.1 2.0</size>
            </box>
          </geometry>
        </visual>

        <inertial>
          <pose>0 0.05 0 0 0 0</pose>
          <mass>50</mass>
          <inertia>
            <ixx>1</ixx>
            <ixy>0</ixy>
            <ixz>0</ixz>
            <iyy>1</iyy>
            <iyz>0</iyz>
            <izz>1</izz>
          </inertia>
        </inertial>
      </link>

      <link name="slidoor_side">
        <pose>-0.6 -1 1 0 0 0</pose>
        <collision name="col1">
          <pose>0 0 0 0 0 0</pose>
          <geometry>
            <box>
              <size>0.1 2.0 2.0</size>
            </box>
          </geometry>
          <surface>
            <bounce>
              <restitution_coefficient>0.2</restitution_coefficient>
              <threshold>0.05</threshold>
            </bounce>
            <contact>
              <ode>
                <max_vel>10</max_vel>
              </ode>
            </contact>
            <friction>
              <ode>
                <mu>5</mu>
                <mu2>5</mu2>
              </ode>
            </friction>
          </surface>
        </collision>

        <visual name="vis1">
          <pose>0 0 0 0 0 0</pose>
          <geometry>
            <box>
              <size>0.1 2.0 2.0</size>
            </box>
          </geometry>
        </visual>

        <inertial>
          <pose>0 0 0 0 0 0</pose>
          <mass>10</mass>
          <inertia>
            <ixx>1</ixx>
            <ixy>0</ixy>
            <ixz>0</ixz>
            <iyy>1</iyy>
            <iyz>0</iyz>
            <izz>1</izz>
          </inertia>
        </inertial>
      </link>

      <link name="slidoor_knob">
        <pose>-0.4 -2.0 1 0 0 0</pose>
        <collision name="col2">
          <pose>0 0 0 0 0 0</pose>
          <geometry>
            <cylinder>
              <radius>0.02</radius>
              <length>0.3</length>
            </cylinder>
          </geometry>
          <surface>
            <bounce>
              <restitution_coefficient>0.2</restitution_coefficient>
              <threshold>0.05</threshold>
            </bounce>
            <contact>
              <ode>
                <max_vel>10</max_vel>
              </ode>
            </contact>
            <friction>
              <ode>
                <mu>1</mu>
                <mu2>1</mu2>
              </ode>
            </friction>
          </surface>
        </collision>

        <visual name="vis2">
          <pose>0 0 0 0 0 0</pose>
          <geometry>
            <cylinder>
              <radius>0.02</radius>
              <length>0.3</length>
            </cylinder>
          </geometry>
        </visual>

        <inertial>
          <pose>0 0 0 0 0 0</pose>
          <mass>2</mass>
          <inertia>
            <ixx>1</ixx>
            <ixy>0</ixy>
            <ixz>0</ixz>
            <iyy>1</iyy>
            <iyz>0</iyz>
            <izz>1</izz>
          </inertia>
        </inertial>
      </link>

      <joint name="slidoor_joint" type="prismatic">
        <parent>slidoor_base</parent>
        <child>slidoor</child>
        <pose>-0.5 0 0 0 0 0</pose>
        <axis>
          <xyz>1 0 0</xyz>
          <dynamics>
            <damping>0.3</damping>
            <friction>0.1</friction>
          </dynamics>
          <limit>
            <lower>0</lower>
            <upper>1.0</upper>
          </limit>
        </axis>
      </joint>

      <joint name="door_fix" type="prismatic">
        <parent>world</parent>
        <child>slidoor_base</child>
        <pose>0 0 0 0 0 0</pose>
        <axis>
          <xyz>0 0 1</xyz>
          <limit>
            <lower>0.05</lower>
            <upper>0.05</upper>
          </limit>
        </axis>
      </joint>

      <joint name="side_fix" type="prismatic">
        <parent>world</parent>
        <child>slidoor_side</child>
        <pose>0 0 0 0 0 0</pose>
        <axis>
          <xyz>0 0 1</xyz>
          <limit>
            <lower>0.05</lower>
            <upper>0.05</upper>
          </limit>
        </axis>
      </joint>

      <joint name="door_knob" type="prismatic">
        <parent>slidoor</parent>
        <child>slidoor_knob</child>
        <pose>0 0 0 0 0 0</pose>
        <axis>
          <xyz>0 1 0</xyz>
          <limit>
            <lower>0.1</lower>
            <upper>0.1</upper>
          </limit>
        </axis>
      </joint>

    </model>
  </world>
</sdf>
